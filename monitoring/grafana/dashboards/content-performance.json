{
  "dashboard": {
    "id": null,
    "title": "AI Content Factory - Content Performance",
    "tags": ["ai-content-factory", "content", "performance"],
    "style": "dark",
    "timezone": "browser",
    "editable": true,
    "gnetId": null,
    "graphTooltip": 0,
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": [
        "30s",
        "1m",
        "5m",
        "15m",
        "30m",
        "1h",
        "2h",
        "1d"
      ]
    },
    "refresh": "1m",
    "version": 1,
    "panels": [
      {
        "id": 1,
        "title": "Content Pipeline Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "content_trends_collected_total",
            "refId": "A",
            "legendFormat": "Trends Collected"
          },
          {
            "expr": "content_opportunities_generated_total",
            "refId": "B",
            "legendFormat": "Opportunities Generated"
          },
          {
            "expr": "content_items_created_total",
            "refId": "C",
            "legendFormat": "Content Created"
          },
          {
            "expr": "content_uploads_completed_total",
            "refId": "D",
            "legendFormat": "Uploads Completed"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 0
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [],
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 10
                },
                {
                  "color": "red",
                  "value": 50
                }
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 2,
        "title": "Content Generation Success Rate",
        "type": "gauge",
        "targets": [
          {
            "expr": "(content_items_created_total / content_opportunities_generated_total) * 100",
            "refId": "A",
            "legendFormat": "Success Rate"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 0,
          "y": 8
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [],
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 50
                },
                {
                  "color": "green",
                  "value": 80
                }
              ]
            },
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        }
      },
      {
        "id": 3,
        "title": "Average Cost per Content",
        "type": "stat",
        "targets": [
          {
            "expr": "content_avg_cost_per_item",
            "refId": "A",
            "legendFormat": "Avg Cost (Baht)"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 8,
          "y": 8
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "area"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [],
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 20
                },
                {
                  "color": "red",
                  "value": 50
                }
              ]
            },
            "unit": "currencyTHB",
            "decimals": 2
          }
        }
      },
      {
        "id": 4,
        "title": "ROI by Platform",
        "type": "bargauge",
        "targets": [
          {
            "expr": "content_roi_youtube",
            "refId": "A",
            "legendFormat": "YouTube"
          },
          {
            "expr": "content_roi_tiktok",
            "refId": "B",
            "legendFormat": "TikTok"
          },
          {
            "expr": "content_roi_instagram",
            "refId": "C",
            "legendFormat": "Instagram"
          },
          {
            "expr": "content_roi_facebook",
            "refId": "D",
            "legendFormat": "Facebook"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 16,
          "y": 8
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "horizontal",
          "displayMode": "gradient"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [],
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 0
                },
                {
                  "color": "green",
                  "value": 100
                }
              ]
            },
            "unit": "percent"
          }
        }
      },
      {
        "id": 5,
        "title": "Trends Collection Over Time",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(content_trends_collected_total[5m]) * 300",
            "refId": "A",
            "legendFormat": "Trends per 5 minutes"
          }
        ],
        "gridPos": {
          "h": 9,
          "w": 12,
          "x": 0,
          "y": 16
        },
        "yAxes": [
          {
            "min": 0,
            "unit": "short"
          },
          {
            "show": false
          }
        ],
        "xAxis": {
          "show": true
        }
      },
      {
        "id": 6,
        "title": "Content Creation Pipeline",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(content_opportunities_generated_total[5m]) * 300",
            "refId": "A",
            "legendFormat": "Opportunities/5min"
          },
          {
            "expr": "rate(content_items_created_total[5m]) * 300",
            "refId": "B",
            "legendFormat": "Content Created/5min"
          },
          {
            "expr": "rate(content_uploads_completed_total[5m]) * 300",
            "refId": "C",
            "legendFormat": "Uploads/5min"
          }
        ],
        "gridPos": {
          "h": 9,
          "w": 12,
          "x": 12,
          "y": 16
        },
        "yAxes": [
          {
            "min": 0,
            "unit": "short"
          },
          {
            "show": false
          }
        ]
      },
      {
        "id": 7,
        "title": "Top Trending Categories",
        "type": "piechart",
        "targets": [
          {
            "expr": "content_trends_by_category",
            "refId": "A",
            "legendFormat": "{{category}}"
          }
        ],
        "gridPos": {
          "h": 9,
          "w": 8,
          "x": 0,
          "y": 25
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "pieType": "pie",
          "tooltip": {
            "mode": "single"
          },
          "legend": {
            "displayMode": "list",
            "placement": "bottom"
          }
        }
      },
      {
        "id": 8,
        "title": "Content Types Distribution",
        "type": "piechart",
        "targets": [
          {
            "expr": "content_types_distribution",
            "refId": "A",
            "legendFormat": "{{content_type}}"
          }
        ],
        "gridPos": {
          "h": 9,
          "w": 8,
          "x": 8,
          "y": 25
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "pieType": "donut",
          "tooltip": {
            "mode": "single"
          },
          "legend": {
            "displayMode": "list",
            "placement": "bottom"
          }
        }
      },
      {
        "id": 9,
        "title": "Platform Upload Success Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "(content_uploads_successful / content_uploads_total) * 100",
            "refId": "A",
            "legendFormat": "Upload Success Rate"
          }
        ],
        "gridPos": {
          "h": 9,
          "w": 8,
          "x": 16,
          "y": 25
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [],
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 80
                },
                {
                  "color": "green",
                  "value": 95
                }
              ]
            },
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        }
      },
      {
        "id": 10,
        "title": "Cost Breakdown",
        "type": "piechart",
        "targets": [
          {
            "expr": "content_cost_ai_text",
            "refId": "A",
            "legendFormat": "AI Text"
          },
          {
            "expr": "content_cost_ai_image",
            "refId": "B",
            "legendFormat": "AI Image"
          },
          {
            "expr": "content_cost_ai_audio",
            "refId": "C",
            "legendFormat": "AI Audio"
          },
          {
            "expr": "content_cost_platform_fees",
            "refId": "D",
            "legendFormat": "Platform Fees"
          },
          {
            "expr": "content_cost_infrastructure",
            "refId": "E",
            "legendFormat": "Infrastructure"
          }
        ],
        "gridPos": {
          "h": 9,
          "w": 12,
          "x": 0,
          "y": 34
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "pieType": "pie",
          "tooltip": {
            "mode": "single"
          },
          "legend": {
            "displayMode": "list",
            "placement": "right"
          }
        }
      },
      {
        "id": 11,
        "title": "Daily Revenue vs Cost",
        "type": "graph",
        "targets": [
          {
            "expr": "sum_over_time(content_daily_cost[1d])",
            "refId": "A",
            "legendFormat": "Daily Cost"
          },
          {
            "expr": "sum_over_time(content_daily_revenue[1d])",
            "refId": "B",
            "legendFormat": "Daily Revenue"
          }
        ],
        "gridPos": {
          "h": 9,
          "w": 12,
          "x": 12,
          "y": 34
        },
        "yAxes": [
          {
            "min": 0,
            "unit": "currencyTHB"
          },
          {
            "show": false
          }
        ],
        "seriesOverrides": [
          {
            "alias": "Daily Cost",
            "color": "#F2495C"
          },
          {
            "alias": "Daily Revenue",
            "color": "#73BF69"
          }
        ]
      },
      {
        "id": 12,
        "title": "Top Performing Content",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, content_performance_views)",
            "refId": "A",
            "format": "table",
            "instant": true
          }
        ],
        "gridPos": {
          "h": 9,
          "w": 24,
          "x": 0,
          "y": 43
        },
        "options": {
          "showHeader": true
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "auto",
              "displayMode": "auto"
            },
            "mappings": [],
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 80
                }
              ]
            }
          },
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "Views"
              },
              "properties": [
                {
                  "id": "custom.displayMode",
                  "value": "color-background"
                },
                {
                  "id": "custom.align",
                  "value": "center"
                }
              ]
            }
          ]
        },
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {},
              "indexByName": {},
              "renameByName": {
                "__name__": "Metric",
                "content_id": "Content ID",
                "platform": "Platform",
                "value": "Views"
              }
            }
          }
        ]
      }
    ],
    "templating": {
      "list": [
        {
          "name": "platform",
          "type": "query",
          "query": "label_values(content_uploads_total, platform)",
          "current": {
            "text": "All",
            "value": "$__all"
          },
          "options": [],
          "includeAll": true,
          "allValue": ".*",
          "multi": true
        },
        {
          "name": "content_type",
          "type": "query", 
          "query": "label_values(content_types_distribution, content_type)",
          "current": {
            "text": "All",
            "value": "$__all"
          },
          "options": [],
          "includeAll": true,
          "allValue": ".*",
          "multi": true
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": "-- Grafana --",
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        },
        {
          "name": "Content Uploads",
          "datasource": "prometheus",
          "enable": true,
          "expr": "increase(content_uploads_completed_total[1m]) > 0",
          "iconColor": "green",
          "tagKeys": "platform",
          "textFormat": "Content uploaded to {{platform}}",
          "titleFormat": "Upload Event"
        }
      ]
    },
    "schemaVersion": 27,
    "version": 1,
    "links": [
      {
        "title": "System Metrics",
        "url": "/d/system-metrics",
        "type": "dashboards"
      },
      {
        "title": "Trend Analysis",
        "url": "/d/trend-analysis", 
        "type": "dashboards"
      }
    ]
  }
}